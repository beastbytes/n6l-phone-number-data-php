{
    "name": "beastbytes/n6l-phone-number-data-php",
    "type": "library",
    "description": "Implementation of n6l-phone-number-data to retrieve national phone number data from PHP storage",
    "keywords": [
        "national phone number",
        "national numbering plan"
    ],
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "Chris Yates",
        "email": "chris.l.yates@gmail.com"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "beastbytes/n6l-phone-number-data": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.",
        "roave/infection-static-analysis-plugin": "^1.0",
        "roave/security-advisories": "dev-latest",
        "spatie/phpunit-watcher": "^1.0",
        "vimeo/psalm": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "BeastBytes\\PhoneNumber\\N6l\\PHP\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeastBytes\\PhoneNumber\\N6l\\PHP\\Tests\\": "tests"
        }
    },
    "scripts": {
        "infection": "roave-infection-static-analysis-plugin",
        "psalm": "psalm",
        "test": "phpunit --testdox",
        "test-watch": "phpunit-watcher watch"
    }
}
